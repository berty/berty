{
  "name": "@berty/web",
  "version": "0.0.1",
  "license": "Beerware",
  "scripts": {
    "postinstall": "mkdir -p node_modules/@berty; ln -fs ../../../common node_modules/@berty",
    "start": "concurrently 'yarn relay --watch' 'node scripts/start.js'",
    "build": "yarn relay && node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "relay": "relay-compiler --src ../common --schema ../common/schema.graphql"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "concurrently": "^3.6.1",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.5.0",
    "loader-utils": "^1.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "object-assign": "4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-dev-utils": "^5.0.2",
    "relay-compiler": "^1.4.1",
    "resolve": "1.8.1",
    "style-loader": "0.22.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "1.1.1",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  }
}
