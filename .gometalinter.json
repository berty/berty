{
  "Vendor": true,
  "Deadline": "2m",
  "Sort": [
    "linter",
    "severity",
    "path",
    "line"
  ],
  "Exclude": [
    ".*\\.pb\\.go",
    ".*\\.gen\\.go",
    "should have comment or be unexported",
    "should have comment \\(or a comment on this block\\) or be unexported",
    "error return value not checked \\(defer",
    "_test.go:"
  ],
  "EnableGC": true,
  "Enable": [
    "deadcode",
    "misspell",
    "structcheck",
    "errcheck",
    "unused",
    "varcheck",
    "staticcheck",
    "unconvert",
    "gofmt",
    "goimports",
    "golint",
    "ineffassign",
    "vet"
  ]
}
